{
  "name": "@genuinegrads/backend",
  "version": "1.0.0",
  "type": "module",
  "description": "GenuineGrads GraphQL API Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "yarn db:generate && tsx watch src/index.ts",
    "build": "yarn db:generate && tsc",
    "start": "node dist/index.js",
    "prestart": "yarn db:generate",
    "db:generate": "yarn db:generate:shared && yarn db:generate:university",
    "db:generate:shared": "prisma generate --schema=prisma/shared.prisma",
    "db:generate:university": "prisma generate --schema=prisma/university.prisma",
    "db:migrate:shared": "prisma migrate dev --schema=prisma/shared.prisma --name",
    "db:migrate:university": "prisma migrate dev --schema=prisma/university.prisma --name",
    "db:push:shared": "prisma db push --schema=prisma/shared.prisma",
    "db:push:university": "prisma db push --schema=prisma/university.prisma",
    "db:studio:shared": "prisma studio --schema=prisma/shared.prisma --port 5555",
    "db:studio:university": "prisma studio --schema=prisma/university.prisma --port 5556",
    "db:seed": "tsx prisma/seed.ts",
    "db:create-uni": "tsx scripts/create-university-db.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@coral-xyz/anchor": "^0.31.1",
    "@metaplex-foundation/mpl-bubblegum": "^1.0.1",
    "@metaplex-foundation/mpl-core": "^1.1.1",
    "@metaplex-foundation/umi": "^0.9.0",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.0",
    "@prisma/client": "^5.9.0",
    "@solana/spl-account-compression": "^0.2.0",
    "@solana/web3.js": "^1.91.0",
    "argon2": "^0.31.2",
    "axios": "^1.6.5",
    "bs58": "^5.0.0",
    "bullmq": "^5.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.5",
    "graphql-tag": "^2.12.6",
    "helius-sdk": "^1.3.4",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.5",
    "pinata-web3": "^0.5.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "qrcode": "^1.5.3",
    "sharp": "^0.34.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "prisma": "^5.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}
